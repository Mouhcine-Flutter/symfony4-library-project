    {{ form_start(form) }}
    <div class="row">
        <div class="col-md-4">{{ form_row(form.nom) }}</div>
    </div>
    <div class="row">
        <div class="col-md-4">{{ form_row(form.prenom) }}</div>
    </div>
    <div class="row">
        <div class="col-md-4">{{ form_row(form.adresse) }}</div>
    </div>
    <div class="row">
        <div class="col-md-4">{{ form_row(form.codepostal) }}</div>
    </div>
    <div class="row">
        <div class="col-md-4">{{ form_row(form.ville) }}</div>
    </div>
    <div class="row">
        <div class="col-md-4">{{ form_row(form.tel) }}</div>
    </div>
    {{ form_widget(form) }}
    <button class="btn btn-primary">{{ button|default('Enregistrer') }}</button>
    {{ form_end(form) }}
